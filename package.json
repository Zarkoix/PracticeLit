{
  "name": "PracticeLit",
  "version": "1.0.0",
  "description": "Server side rendering template for React 16",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node src/index",
    "watch": "cross-env NODE_ENV=development babel-watch src/index",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "cross-env NODE_ENV=production webpack -p --config webpack/client.production.js",
    "build:server": "cross-env NODE_ENV=production webpack -p --config webpack/server.production.js",
    "build:node": "cross-env NODE_ENV=production webpack -p --config webpack/node.production.js"
  },
  "keywords": [
    "ssr",
    "react",
    "fiber",
    "server side rendering",
    "express",
    "ejs"
  ],
  "author": {
    "name": "Adam Towers",
    "email": "ajtowers@uw.edu"
  },
  "license": "NONE",
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-0"
    ],
    "plugins": [
      "react-loadable/babel"
    ]
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "babel-polyfill": "^6.26.0",
    "brace": "^0.11.1",
    "ejs": "^2.5.7",
    "express": "^4.16.0",
    "react": "^16.2.0",
    "react-ace": "^5.9.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-universal-component": "^2.5.5",
    "styled-components": "^3.2.3",
    "webpack-flush-chunks": "^1.1.23",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.2.1",
    "cross-env": "^5.0.5",
    "css-hot-loader": "^1.3.1",
    "css-loader": "^0.28.11",
    "extract-css-chunks-webpack-plugin": "^2.0.17",
    "extract-text-webpack-plugin": "^3.0.0",
    "prettier": "^1.11.1",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-hot-server-middleware": "^0.3.2",
    "webpack-merge": "^4.1.1"
  }
}
