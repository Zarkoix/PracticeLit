{
  "name": "PracticeLit",
  "version": "1.0.0",
  "description": "Server side rendering template for React 16",
  "main": "src/index.ts",
  "scripts": {
    "start": "NODE_ENV=development PORT=3000 RMQ=localhost babel-node --extensions '.js,.jsx,.ts,.tsx' src/index.ts",
    "watch": "NODE_ENV=development babel-watch src/index",
    "build": "npm run build:client && npm run build:server && npm run build:node",
    "build:client": "NODE_ENV=production webpack -p --config webpack/client.production.js",
    "build:server": "webpack -p --config webpack/server.production.js",
    "build:node": "webpack -p --config webpack/node.production.js",
    "setup": "npm i"
  },
  "keywords": [
    "ssr",
    "react",
    "fiber",
    "server side rendering",
    "express",
    "ejs"
  ],
  "author": {
    "name": "Adam Towers",
    "email": "ajtowers@uw.edu"
  },
  "license": "NONE",
  "dependencies": {
    "amqplib": "^0.5.2",
    "babel-polyfill": "^6.26.0",
    "brace": "^0.11.1",
    "ejs": "^2.5.7",
    "express": "^4.16.0",
    "react": "^16.2.0",
    "react-ace": "^5.9.0",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-transition-group": "^2.3.0",
    "react-universal-component": "^2.8.4",
    "styled-components": "^3.2.3",
    "winston": "^2.3.1",
    "ws": "^5.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.44",
    "@babel/core": "7.0.0-beta.44",
    "@babel/node": "7.0.0-beta.44",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "@babel/preset-typescript": "^7.0.0-beta.44",
    "@types/amqplib": "^0.5.7",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.11.1",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "@types/react-router": "^4.0.23",
    "@types/winston": "^2.3.9",
    "@types/ws": "^4.0.2",
    "amqp-ts": "^1.4.0",
    "awesome-typescript-loader": "^5.0.0",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-universal-import": "^1.4.0",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.2.1",
    "cross-env": "^5.0.5",
    "css-hot-loader": "^1.3.1",
    "css-loader": "^0.28.11",
    "extract-css-chunks-webpack-plugin": "^2.0.18",
    "extract-text-webpack-plugin": "^3.0.2",
    "prettier": "^1.11.1",
    "stats-webpack-plugin": "^0.6.2",
    "style-loader": "^0.18.2",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-flush-chunks": "^1.2.3",
    "webpack-hot-middleware": "^2.21.0",
